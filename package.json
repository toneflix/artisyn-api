{
  "name": "artisyn-api",
  "version": "1.0.0",
  "description": "Backend API for Artisyn - a decentralized protocol connecting local artisans with users",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "migrate": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "prisma:generate": "prisma generate",
    "seed": "ts-node src/database/seed.ts",
    "command": "ts-node src/commands/index.ts",
    "initialize": "prisma generate && prisma migrate dev && ts-node src/database/seed.ts",
    "postinstall": "prisma generate"
  },
  "_moduleAliases": {
    "src": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toneflix/artisyn-api.git"
  },
  "keywords": [
    "artisans",
    "starknet",
    "decentralized",
    "marketplace"
  ],
  "author": "Toneflix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toneflix/artisyn-api/issues"
  },
  "homepage": "https://github.com/toneflix/artisyn-api#readme",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/method-override": "^3.0.0",
    "argon2": "^0.41.1",
    "base64url": "^3.0.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-http": "^10.4.0",
    "simple-body-validator": "^1.3.9",
    "ua-parser-js": "^2.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "latest",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6",
    "@types/passport": "^0",
    "@types/passport-facebook": "^3",
    "@types/passport-google-oauth20": "^2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "commander": "^13.1.0",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "prisma": "^6.8.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
